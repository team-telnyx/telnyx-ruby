module Telnyx
  module Models
    type task_status = :pending | :starting | :running | :completed | :failed

    module TaskStatus
      extend Telnyx::Internal::Type::Enum

      PENDING: :pending
      STARTING: :starting
      RUNNING: :running
      COMPLETED: :completed
      FAILED: :failed

      def self?.values: -> ::Array[Telnyx::Models::task_status]
    end
  end
end
